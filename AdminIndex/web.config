<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <staticContent>
            <remove fileExtension=".323" />
            <mimeMap fileExtension=".*" mimeType="application/octet-stream" />
        </staticContent>
        <httpProtocol>
            <customHeaders>
                <remove name="X-Powered-By" />
                <add name="X-Powered-By" value="PHP5.6.9" />
            </customHeaders>
        </httpProtocol>
    </system.webServer>
</configuration>
